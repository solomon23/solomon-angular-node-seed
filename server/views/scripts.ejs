<% if (env === "production") {%>
    <script src="<%- config.cdn %><%= map.get('/js/vendor.js')%>" type="text/javascript"></script>
    <script src="<%- config.cdn %><%= map.get('/js/combined.js')%>" type="text/javascript"></script>
<%}else{%>
    <!-- build:js js/vendor.js -->
    <script src="bower/jquery/dist/jquery.js"></script>
    <script src="bower/angular/angular.js"></script>
    <script src="bower/angular-classy/angular-classy.min.js"></script>
    <script src="bower/bootstrap/dist/js/bootstrap.js"></script>
    <script src="bower/angular-resource/angular-resource.js"></script>
    <script src="bower/angular-cookies/angular-cookies.js"></script>
    <script src="bower/angular-sanitize/angular-sanitize.js"></script>
    <script src="bower/angular-route/angular-route.js"></script>
    <script src="bower/fastclick/lib/fastclick.js"></script>
    <script src="bower/underscore/underscore.js"></script>
    <!-- endbuild -->

    <!-- build:js js/combined.js -->

    <!-- angular setup -->
    <script src="js/config.js"></script>
    <script src="js/app.js"></script>
    <script src="js/routes.js"></script>
    <script src="js/templates.js"></script>

    <!-- services -->
    <script src="js/services/config.js"></script>
    <script src="js/services/remote.js"></script>
    <script src="js/services/version.js"></script>
    <script src="js/services/auth.js"></script>

    <!-- directives -->
    <script src="js/directives/app-version.js"></script>

    <!-- filters -->
    <script src="js/filters/interpolate.js"></script>
    <script src="js/filters/static.js"></script>

    <!-- controllers -->
    <script src="js/controllers/root.js"></script>
    <script src="js/controllers/page1.js"></script>
    <script src="js/controllers/page2.js"></script>
    <script src="js/controllers/admin.js"></script>
    <script src="js/controllers/login.js"></script>
    <!-- endbuild -->
<%}%>
